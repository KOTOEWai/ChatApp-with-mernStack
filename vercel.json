{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "fronted/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/fronted/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}